{
    "version": 2,
    "builds": [
      { "src": "dev_project/wsgi.py", "use": "@vercel/python" },
      { "src": "static/**/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/(.*)", "dest": "dev_project/wsgi.py" }
    ],
    "projectSettings": {
      "framework": {
        "version": "3"
      }
    }
  }
  